{
  "name": "id3-parser",
  "version": "1.5.0",
  "description": "A pure JavaScript id3 tag parser.",
  "main": "lib/parser.js",
  "typings": "index.d.ts",
  "files": [
    "dest",
    "lib",
    "index.d.ts"
  ],
  "scripts": {
    "lint": "grunt lint",
    "build": "grunt build",
